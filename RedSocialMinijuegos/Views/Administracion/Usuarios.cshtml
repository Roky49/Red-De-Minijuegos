@model IEnumerable<RedSocialMinijuegos.Models.Usuario>

@{
    ViewBag.Title = "Usuarios";
}

<h2>Usuarios</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.UltimaConexion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Usuario1)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.Roles)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
     
        <td>
            @Html.DisplayFor(modelItem => item.UltimaConexion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario1)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Roles)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditarUsuarios", new { id=item.IdUsuario }) |
           
            @Html.ActionLink("Delete", "BorrarUsuarios", new { id=item.IdUsuario })
        </td>
    </tr>
}

</table>
